{
  "name": "backend",
  "version": "1.0.0",
  "description": "that's the backend of mecanico app",
  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "node prisma/seed.js",
    "daily:appointments": "node -r esbuild-register src/scripts/dailyAppointmentMaintenance.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": [],
  "author": "José Jesús Ibañez Cantos",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "esbuild-register": "^3.6.0",
    "nodemon": "^3.1.11",
    "prisma": "^4.16.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
